			+ &apos;|&apos; + hl7fields[16]  //PID-22: Ethnic Group
			+ &apos;\n&apos;
			//PV1 Segment
			+ &apos;PV1|&apos; + hl7fields[17]    //PV1-1: Set ID - PV1 (1)
